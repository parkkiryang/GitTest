import java.util.Arrays;
import java.util.Scanner;

//두 정수 입력받아서 평균을 구하는 프로그램
public class Ex02 {

	public static void main(String[] args) { // jvm 호출
		

		int num=10;
		
		//--num ->  num = num - 1,  num-=1
		int i = num--;  //i(10), num(9)
		int j = --num; //num(8), j(8)
		
		System.out.println(i + "," + j  + ","+ num);
		
	}
	

}

