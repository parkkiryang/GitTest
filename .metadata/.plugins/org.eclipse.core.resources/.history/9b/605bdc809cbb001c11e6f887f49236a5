import java.util.Arrays;
import java.util.Scanner;

//두 정수 입력받아서 평균을 구하는 프로그램
public class Ex02 {

	public static void main(String[] args) { // jvm 호출

		//int 배열 1~100 사이 값을 랜덤하게 입력받아서 총합 /평균 구하기
		
		int num = 250;
		String str = "java";
		char ch = 'm';
		System.out.printf("%d\n", num);
		System.out.printf("%o\n", num);
		System.out.printf("%x\n", num);
		System.out.printf("%X\n", num);

		System.out.printf("%s\n", str);
		
		System.out.printf("%d\n", ch);
		
		System.out.printf("%s\n", Integer.toBinaryString(num));
		
		
	}

	
}
